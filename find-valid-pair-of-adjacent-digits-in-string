class Solution:
    def findValidPair(self, s: str) -> str:
        if len(s) <= 2:
            return ""
        l=0
        r=1
        cnt = Counter(s)
        while r < len(s):
            if s[l]!= s[r] and cnt[s[l]] == int(s[l]) and cnt[s[r]] == int(s[r]):
                return s[l]+s[r]
            l+=1
            r+=1
        return ""

         
