class Solution:
    def romanToInt(self, s: str) -> int:
        arr={'I':1, 'V': 5, 'X': 10, 'L': 50, 'C': 100, 'D': 500, 'M': 1000}
        num =0
        for i in range(len(s)):
            if i+1 < len(s) and arr[s[i]] < arr[s[i+1]]:
                num-= arr[s[i]]
            else:
                num+= arr[s[i]]
       
        return num
