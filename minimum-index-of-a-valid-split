class Solution:
    def minimumIndex(self, nums: List[int]) -> int:
        mcnt= Counter(nums)
        m=0
        mm=0
        l=len(nums)
        for i in mcnt:
            if mcnt[i]>=m:
                m=mcnt[i]
                mm=i
        c=0
        ans=-1
        for i in range(l):
            if nums[i] ==mm:
                c+=1
                if c*2 > i+1 and (m-c)*2 > l-(i+1):
                    ans=i
                    break
        return ans

